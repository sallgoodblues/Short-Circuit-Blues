<?xml version="1.0" encoding="utf-8"?><Defs>

  <SoundDef>
    <defName>SrZzztMap</defName>
    <context>MapOnly</context>
    <maxSimultaneous>2</maxSimultaneous>
    <subSounds>
      <li>
        <grains>
          <li Class="AudioGrain_Folder">
            <clipFolderPath>Sr_Sounds/MapZzzt</clipFolderPath>
          </li>
        </grains>
        <volumeRange>10~70</volumeRange>
        <distRange>10~130</distRange>
      </li>
    </subSounds>
  </SoundDef>
  
  <SoundDef>
    <defName>SrZzztIn</defName>    
